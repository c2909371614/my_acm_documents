#include<bits/stdc++.h>
#define ll long long
using namespace std;
const int maxM=1e5+5;
int  main() {
	int T;
	int a[maxM],n;
	cin>>T;
	while(T--) {
		cin>>n;
		for(int i=0; i<n; i++) {
			cin>>a[i];
		}
		
		for(int j=0; j<n-1; j++) {
			int min=j;
			for(int i=j+1; i<n; i++)
				if(a[i]<a[min])
					min=i;
		if(min!=j){ 
			int temp=a[i];
			a[i]=a[min];
			a[min]=temp;
		} 
		}
	}
}
